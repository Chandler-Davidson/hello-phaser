{
    "name": "hello-phaser",
    "description": "A Phaser 3 template using bun to publish to npm.",
    "version": "1.1.14",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/chandler-davidson/hello-phaser.git"
    },
    "author": "Chandler Davidson",
    "license": "MIT",
    "licenseUrl": "http://www.opensource.org/licenses/mit-license.php",
    "bugs": {
        "url": "https://github.com/phaserjs/template-bun/issues"
    },
    "homepage": "https://github.com/phaserjs/template-bun#readme",
    "scripts": {
        "dev": "bunx --bun vite --config vite/config.dev.mjs",
        "build": "bunx --bun vite build --config vite/config.prod.mjs",
        "prepublishOnly": "bun run build && bun pm version patch"
    },
    "devDependencies": {
        "typescript": "~5.7.2",
        "vite": "^6.3.1",
        "terser": "^5.39.0"
    },
    "dependencies": {
        "phaser": "^3.90.0"
    }
}